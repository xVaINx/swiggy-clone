{
  "expo": {
    "name": "swiggy-app",
    "slug": "swiggy-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/swiggy-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/swiggy-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/swiggy-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.xvainx.swiggyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/swiggy-logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2bd1aedf-bf9c-4680-b513-ed34dc043448"
      }
    }
  }
}
